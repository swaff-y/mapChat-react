v className=\"sidebarChat_info\">\n        <h2>{ props.room.name }</h2>\n        <p>{ lastMessage }\n        </p>\n      </div>\n    </div>\n  )\n}\n\nexport default SidebarChat;\n","/Users/kyleswaffield/sei40/projects/mapChat/whatsapp/src/Home.js",["92","93","94","95","96","97"],"/Users/kyleswaffield/sei40/projects/mapChat/whatsapp/src/login/Login.js",[],"/Users/kyleswaffield/sei40/projects/mapChat/whatsapp/src/chat/Emoji.js",["98","99"],"import React, {useEffect, useState} from 'react';\nimport Picker from 'emoji-picker-react';\n\nconst Emoji = (props) => {\n\n  const onEmojiClick = (event, emojiObject) => {\n    props.getEmoji(emojiObject);\n    props.handleToggle();\n  };\n\n  return(\n    <div>\n       <Picker onEmojiClick={onEmojiClick} />\n    </div>\n  )\n}\n\nexport default Emoji;\n\n// {chosenEmoji ? (\n//   <span>You chose: {chosenEmoji.emoji}</span>\n// ) : (\n//   <span>No emoji Chosen</span>\n// )}\n",{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"100","replacedBy":"104"},{"ruleId":"102","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":4,"column":10,"nodeType":"108","messageId":"109","endLine":4,"endColumn":14},{"ruleId":"110","severity":1,"message":"111","line":21,"column":5,"nodeType":"112","endLine":21,"endColumn":21,"suggestions":"113"},{"ruleId":"106","severity":1,"message":"114","line":1,"column":16,"nodeType":"108","messageId":"109","endLine":1,"endColumn":25},{"ruleId":"106","severity":1,"message":"115","line":1,"column":27,"nodeType":"108","messageId":"109","endLine":1,"endColumn":35},{"ruleId":"110","severity":1,"message":"116","line":18,"column":5,"nodeType":"112","endLine":18,"endColumn":7,"suggestions":"117"},{"ruleId":"110","severity":1,"message":"118","line":24,"column":5,"nodeType":"112","endLine":24,"endColumn":24,"suggestions":"119"},{"ruleId":"106","severity":1,"message":"120","line":11,"column":7,"nodeType":"108","messageId":"109","endLine":11,"endColumn":16},{"ruleId":"106"